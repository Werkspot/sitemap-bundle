{
    "name": "werkspot/sitemap-bundle",
    "description": "Bundle for generating dynamic sitemap.xml content with support for multiple sections",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": [
        "seo",
        "sitemap",
        "symfony",
        "bundle"
    ],
    "autoload": {
        "psr-4": {
            "Werkspot\\Bundle\\SitemapBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Werkspot\\Bundle\\SitemapBundle\\Tests\\": "Tests"
        },
        "classmap": ["Tests/Controller/app/AppKernel.php"]
    },
    "authors": [
        {
            "name": "Werkspot Developers"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/framework-bundle": "^5.2 || ^6.3"
    },
    "require-dev": {
        "ext-simplexml": "*",
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^9.5",
        "symfony/finder": "^5.2 || ^6.3",
        "symfony/asset": "^5.2 || ^6.3",
        "symfony/templating": "^5.2 || ^6.3",
        "symfony/browser-kit": "^5.2 || ^6.3",
        "symfony/twig-bridge": "^5.2 || ^6.3",
        "symfony/twig-bundle": "^5.2 || ^6.3",
        "symfony/yaml": "^5.2 || ^6.3",
        "instapro/coding-standard": "^1.0"
    },
    "config": {
        "bin-dir": "vendor/bin"
    }
}
