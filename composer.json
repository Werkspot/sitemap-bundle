{
    "name": "werkspot/sitemap-bundle",
    "description": "Bundle for generating dynamic sitemap.xml content with support for multiple sections",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": [
        "seo",
        "sitemap",
        "symfony",
        "bundle"
    ],
    "autoload": {
        "psr-4": { "Werkspot\\Bundle\\SitemapBundle\\": "" }
    },
    "authors": [
        {
            "name": "Thanos Polymeneas",
            "email": "zinbiel@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "symfony/framework-bundle": "~2.3|~3.0"
    },
    "require-dev": {
        "mockery/mockery": "~0.9|^1.0@dev",
        "phpunit/phpunit": "~4.8|^5.7",
        "symfony/finder": "~2.3|~3.0",
        "symfony/asset": "~2.3|~3.0",
        "symfony/templating": "~2.3|~3.0",
        "symfony/browser-kit": "~2.3|~3.0",
        "symfony/twig-bridge": "~2.3|~3.0",
        "symfony/twig-bundle": "~2.3|~3.0"
    },
    "config": {
        "bin-dir": "bin"
    }
